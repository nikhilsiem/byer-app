<script type="text/javascript">


  
$(document).ready(function(){

  cTran=0;
  cDash=1;


$('.dashboard').click().css("background-color", "#1284F0");



  $('.tranction').click(function(){
    cTran++;
    if(cTran%2!=0){
      $('.tranction').css("background-color", "#1284F0");
      if(cDash%2!=0){cDash++;$('.dashboard').css("background-color", "white");}
    }else{
      $('.tranction').css("background-color", "white");
    }
   
    $('#partner-dashboard').collapse('hide')
  });





  $('.dashboard').click(function(){
    
     cDash++;
    if(cDash%2!=0){

      $('.dashboard').css("background-color", "#1284F0");
      if(cTran%2!=0){cTran++;$('.tranction').css("background-color", "white");}

    }else{
      $('.dashboard').css("background-color", "white");
    }
    $('#transcation-panel').collapse('hide')
  });

});

</script>



