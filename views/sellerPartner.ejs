}

    function changecat(value) {
        if (value.length == 0) document.getElementById("category").innerHTML = "<option></option>";
        else {
            var catOptions = "";
            for (categoryId in productsByCategory[value]) {
                catOptions += "<option value=\""+productsByCategory[value][categoryId]+"\">" + productsByCategory[value][categoryId] + "</option>";
            }
            document.getElementById("category").innerHTML = catOptions;
        }
    }




</script>


